import java.util.*;
public class loopreturn {
    public static void main(String[] args) {
        int a=7;
        for(int i = 2 ; i < 15 ; i++){
            a=a+i;
            if(a == 1)
            return;
            else
            System.out.println(a);
        }
        a = a+ 10;
        System.out.println(a*2);
    }
}
